{
  "name": "@iterate-com/os",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "doppler run -- vite build",
    "deploy:prd": "doppler run --config prd -- sh -c 'tsx ./alchemy.run.ts cli deploy --stage \"$APP_STAGE\"'",
    "deploy:stg": "doppler run --config stg -- sh -c 'tsx ./alchemy.run.ts cli deploy --stage \"$APP_STAGE\"'",
    "deploy": "pnpm deploy:${APP_STAGE}",
    "dev": "doppler run -- sh -c 'tsx ./alchemy.run.ts cli --dev --stage dev-$ITERATE_USER'",
    "dev:tunnel": "DEV_TUNNEL=1 pnpm dev",
    "dev:vite": "vite",
    "preview": "pnpm run build && vite preview",
    "test": "vitest",
    "typecheck": "tsc -b",
    "db:reset": "doppler run -- tsx -e \"(async () => { const postgres = (await import('postgres')).default; const sql = postgres('postgres://postgres:postgres@localhost:5432/postgres'); await sql.unsafe('DROP DATABASE IF EXISTS os'); await sql.unsafe('CREATE DATABASE os'); await sql.end(); })()\" && pnpm db:migrate",
    "super-reset": "pnpm db:reset",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:studio:prd": "PSCALE_DATABASE_URL=$(doppler secrets --config prd get --plain PLANETSCALE_PROD_POSTGRES_URL) drizzle-kit studio",
    "docker:build": "doppler run -- tsx sandbox/build-docker-image.ts",
    "docker:build:test": "doppler run -- sh -c 'RUN_LOCAL_DOCKER_TESTS=true pnpm vitest run --reporter=verbose sandbox/local-docker.test.ts'",
    "docker:shell": "doppler run -- tsx sandbox/local-docker-shell.ts",
    "daytona:build": "doppler run -- tsx sandbox/push-docker-image-to-daytona.ts",
    "billing:generate": "tsx scripts/generate-billing-meters.ts"
  },
