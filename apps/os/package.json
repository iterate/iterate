{
  "name": "@iterate-com/os",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "doppler run -- vite build",
    "deploy": "doppler run -- tsx ./alchemy.run.ts cli deploy --stage \"${STAGE:-local-${USER}}\"",
    "posthog:sourcemaps:inject": "doppler run -- posthog-cli --host https://eu.posthog.com sourcemap inject --directory ./build",
    "posthog:sourcemaps:upload": "doppler run -- posthog-cli --host https://eu.posthog.com sourcemap upload --directory ./build",
    "dev": "doppler run -- tsx ./alchemy.run cli --dev --stage local-$USER",
    "eval": "doppler run -- evalite run-once && tsx evals/evalite-exporter.ts",
    "eval:serve": "pnpx serve ignoreme/evalite-ui",
    "eval:watch": "doppler run -- evalite watch",
    "iterate": "doppler run -- tsx --import ./sdk/cli/cf-shim/loader.ts ./sdk/cli/index.ts",
    "preview": "pnpm run build && vite preview",
    "test": "doppler run -- vitest",
    "test:watch": "doppler run -- vitest --watch",
    "typecheck": "tsc -b",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:query": "docker exec -it postgres-iterate psql -U postgres -d iterate",
    "db:query:file": "docker exec -i postgres-iterate psql -U postgres -d iterate -f",
    "db:exec": "docker exec -i postgres-iterate psql -U postgres -d iterate -c"
  },
