{
  "name": "@iterate-com/os",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types --no-include-runtime",
    "deploy": "doppler run -- pnpm run build && doppler run -- pnpm run posthog:sourcemaps && doppler run -- wrangler deploy",
    "posthog:sourcemaps": "posthog-cli --host https://eu.posthog.com sourcemap inject --directory ./build && posthog-cli --host https://eu.posthog.com sourcemap upload --directory ./build",
    "dev": "pnpm iterate dev start",
    "eval": "doppler run -- evalite run-once && tsx evals/evalite-exporter.ts",
    "eval:serve": "pnpx serve ignoreme/evalite-ui",
    "eval:watch": "doppler run -- evalite watch",
    "iterate": "doppler run -- tsx --import ./sdk/cli/cf-shim/loader.ts ./sdk/cli/index.ts",
    "preview": "pnpm run build && vite preview",
    "test": "doppler run -- vitest",
    "test:onboarding": "VITEST_RUN_ONBOARDING_TEST=true doppler run -- vitest run ./backend/e2e-onboarding.test.ts",
    "test:onboarding:production": "VITEST_RUN_ONBOARDING_TEST=true doppler run --config prd -- vitest run ./backend/e2e-onboarding.test.ts",
    "test:watch": "doppler run -- vitest --watch",
    "typecheck": "pnpm run cf-typegen && react-router typegen && tsc -b",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
