{
  "name": "@iterate-com/os2",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "doppler run -- vite build",
    "deploy": "doppler run -- tsx ./alchemy.run.ts cli deploy --stage \"${STAGE:-local-${ITERATE_USER}}\"",
    "dev": "doppler run -- tsx ./alchemy.run.ts cli --dev --stage local-$ITERATE_USER",
    "dev:vite": "vite",
    "preview": "pnpm run build && vite preview",
    "test": "vitest",
    "test:e2e": "doppler run -- playwright test",
    "typecheck": "tsc -b",
    "db:reset": "doppler run -- tsx -e \"(async () => { const postgres = (await import('postgres')).default; const sql = postgres('postgres://postgres:postgres@localhost:5432/postgres'); await sql.unsafe('DROP DATABASE IF EXISTS os2'); await sql.unsafe('CREATE DATABASE os2'); await sql.end(); })()\" && pnpm db:migrate",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "sandbox:snapshot": "doppler run -- tsx sandbox/snapshot.ts"
  },
